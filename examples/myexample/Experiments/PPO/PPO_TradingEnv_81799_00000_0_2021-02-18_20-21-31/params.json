{
  "clip_rewards": false,
  "entropy_coeff": 0.1,
  "env": "TradingEnv",
  "env_config": {
    "window_size": 30
  },
  "framework": "torch",
  "gamma": 0,
  "ignore_worker_failures": true,
  "lambda": 0.72,
  "log_level": "DEBUG",
  "lr": 5e-05,
  "model": {
    "_time_major": false,
    "fcnet_hiddens": [
      32,
      16,
      16
    ],
    "lstm_cell_size": 32,
    "lstm_use_prev_action": false,
    "lstm_use_prev_reward": false,
    "max_seq_len": 20,
    "use_lstm": true,
    "vf_share_layers": false
  },
  "num_gpus": 1,
  "num_workers": 3,
  "observation_filter": "MeanStdFilter",
  "vf_loss_coeff": 1.0
}